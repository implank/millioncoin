<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MillionCoins">

    <application
        android:name="com.example.MillionCoins.UniteApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.MillionCoins.MonthChartActivity"></activity>
        <activity android:name="com.example.MillionCoins.SettingActivity" />
        <activity android:name="com.example.MillionCoins.HistoryActivity" />
        <activity android:name="com.example.MillionCoins.AboutActivity" />
        <activity android:name="com.example.MillionCoins.SearchActivity" />
        <activity android:name="com.example.MillionCoins.RecordActivity" />
        <activity android:name="com.example.MillionCoins.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>